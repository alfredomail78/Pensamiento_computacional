Algoritmo consumo_de_combustible
	definir nombre Como Caracter
	definir kilometros Como numerico
	definir consumo Como numerico
	definir rendimiento como numerico
	rendimiento <- 20.8
	escribir " Hola, ¿cuál es tu nombre?"
	leer nombre
	Escribir nombre, " señale los kilómetros a recorrer" 
	Leer kilometros
	consumo <- kilometros/rendimiento
	Escribir nombre, " durante su recorrido consumirá: ", consumo, " litros"
	
	
	
	
FinAlgoritmo